<%- include('../../partials/header_html'); %>

<style>
td a {
   color:blue;
}
.card-body a {
   color:black;
}
</style>
<body>
  <div id='wrapper'>		
	    
	    
	<div id='header'>
       <%- include('../../partials/header_img'); %>
    </div>
	
    <%- include('../../partials/navbar'); %>
    
    <div id='' class='page-content' >
      <div class='jumbotron'> 
	
	  					
		 
		 ORAL: <%= oral %>
		 
		 
		<center>
		 <h4 class='title text-info'>Human Oral Microbiome Taxonomic Level List</h4>
		 
		 <% if(oral){ %>
		   Note: this table includes non-oral/nasal reference taxa, 
		   <a href='tax_level' >[show oral/nasal only]</a>
		 <% }else{ %>
		   Note: this table does not includes non-oral/nasal reference taxa, 
		   <a href='tax_level' >[show also non-oral/nasal taxa]</a>
		 <% }%>
		 
		 
	<div class="card text-white bg-secondary mb-3" style="max-width: 50rem;">
  		<h4 class="card-header">Taxonomic Level:</h4>
  		<div class="card-body">
    	   <p class="card-text">
   
            Select Rank: &nbsp; &nbsp; &nbsp;    
            <a href='#' id='Domain' onclick="change_level('Domain')">Domain</a> 
            &gt; <a href='#' id='Phylum' onclick="change_level('Phylum')">Phylum</a> 
            &gt; <a href='#' id='Class' onclick="change_level('Class')">Class</a> 
            &gt; <a href='#' id='Order' onclick="change_level('Order')">Order</a> 
            &gt; <a href='#' id='Family' onclick="change_level('Family')">Family</a> 
            &gt; <a href='#' id='Genus' onclick="change_level('Genus')">Genus</a> 
            &gt; <a href='#' id='Species' onclick="change_level('Species')">Species</a>
		   </p>
        </div>
     </div>
		 
		 
		 <br>
		 <div id='taxlevel_tdiv2' style='background: lightgrey;'></div>
	    </center> 
	  </div> <!-- End jumbo -->
    </div> <!-- end: page-content -->
    <div id='footer'>
         <%- include('../../partials/footer'); %>
    </div>
       
   
  </div> <!-- end wrapper -->
</body>

</html>

<script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
<script type="text/javascript" src="/js/taxonomy.js"></script>
<script type="text/javascript" src="/js/bootstrap.js"></script>
<script>
  $(document).ready(function(){                    
   
    change_level('Domain')
});

</script>

