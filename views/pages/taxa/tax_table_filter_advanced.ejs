            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='enviro' name='enviro' onclick='update_sb()' \> Environmental&nbsp;
            <% } %>
            <% if(statusfltr.site.enviro_air === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='enviro_air' name='enviro_air' onclick='update_sb()' \> Environmental_air&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='enviro_air' name='enviro_air' onclick='update_sb()' \> Environmental_air&nbsp;
            <% } %>
            <% if(statusfltr.site.enviro_food === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='enviro_food' name='enviro_food' onclick='update_sb()' \> Environmental_food&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='enviro_food' name='enviro_food' onclick='update_sb()' \> Environmental_food&nbsp;
            <% } %>
            <% if(statusfltr.site.enviro_sewage === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='enviro_sewage' name='enviro_sewage' onclick='update_sb()' \> Environmental_sewage&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='enviro_sewage' name='enviro_sewage' onclick='update_sb()' \> Environmental_sewage&nbsp;
