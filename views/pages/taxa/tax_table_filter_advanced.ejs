            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='oral_high' name='oral_high' onclick='update_sb()'\> Oral (high)&nbsp;
            <% } %>
            <% if(statusfltr.site.oral_caries === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='oral_caries' name='oral_caries' onclick='update_sb()'\> Oral (caries)&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='oral_caries' name='oral_caries' onclick='update_sb()'\> Oral (caries)&nbsp;
            <% } %>
            <% if(statusfltr.site.oral_perio === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='oral_perio' name='oral_perio' onclick='update_sb()'\> Oral (periodontitis)&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='oral_perio' name='oral_perio' onclick='update_sb()'\> Oral (periodontitis)&nbsp;
            <% } %>
            
            <br>
           
            
            <% if(statusfltr.site.nasal === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='nasal' name='nasal' onclick='update_sb()' \> Nasal&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='nasal' name='nasal' onclick='update_sb()' \> Nasal&nbsp;
            <% } %>
            <% if(statusfltr.site.nasal_low === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='nasal_low' name='nasal_low' onclick='update_sb()' \> Nasal (low)&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='nasal_low' name='nasal_low' onclick='update_sb()' \> Nasal (low)&nbsp;
            <% } %>
            <% if(statusfltr.site.nasal_scarce === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='nasal_scarce' name='nasal_scarce' onclick='update_sb()' \> Nasal (scarce)&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='nasal_scarce' name='nasal_scarce' onclick='update_sb()' \> Nasal (scarce)&nbsp;
            <% } %>

            <% if(statusfltr.site.skin === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='skin' name='skin' onclick='update_sb()' \> Skin&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='skin' name='skin' onclick='update_sb()' \> Skin&nbsp;
            <% } %>
            
            <% if(statusfltr.site.gut === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='gut' name='gut' onclick='update_sb()' \> Gut&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='gut' name='gut' onclick='update_sb()' \> Gut&nbsp;
            <% } %>
            
            <% if(statusfltr.site.vaginal === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='vaginal' name='vaginal' onclick='update_sb()' \> Vaginal&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='vaginal' name='vaginal' onclick='update_sb()' \> Vaginal&nbsp;
            <% } %>
            
            <br>
            
