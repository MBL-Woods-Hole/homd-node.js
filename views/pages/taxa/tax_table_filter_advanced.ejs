<style>
.letter{
  cursor:pointer;
  color:blue;
}
.noletter{
  cursor:pointer;
  color:blue;
}
.alpha-list{
   font-family: monospace;
}
#filter_form_div{
   border:1px solid black;
   padding:2px;
}
#form_btn{
  width:100%;
  color:grey;
  
}
#tax_filter_form{
   padding:0 40px;
}
#main_table td{
   padding:0 30px;
   
   
}
#filter_table{
   
   border-collapse:colapse;
}
#filter_table td{
   border-top:1px solid brown;
   padding: 8px 3px 1px 3px;
}
#reset_div a{
   font-size: 14px;
}
#sort_rev{
   
   border: 1px solid brown;
   padding:2px 1px 1px 1px;
   margin:1px;
}
.button{
   cursor:pointer;
   color:blue;
   background:lightgrey;
}
#reset{
   cursor:default;
   color:grey;
   background:lightgrey;
}
</style>
<%- include('../../partials/header_html'); %>

<body>
    <div id='wrapper'>
    <%- include('../../partials/header_img'); %>
    <%- include('../../partials/navbar'); %>
    
    
    <div class='page-content' >
              
       <div class='title'>HOMD Taxon Table Search</div>
    
     <div id='filter_form_div'>

<form id='tax_filter_form' name='tax_filter_form' method='POST' action='tax_table'>
<div id='reset_div'>
  <a class='button' id='reset' href='reset_ttable'>Reset Filters</a> 
</div> 

<table id='main_table'>
<tr>
<table id='filter_table' border='1'>
