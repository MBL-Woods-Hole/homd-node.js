            <% if(statusfltr.site.enviro_soil === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='enviro_soil' name='enviro_soil' onclick='update_sb()' \> Environmental (soil)&nbsp;
            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='enviro_soil' name='enviro_soil' onclick='update_sb()' \> Environmental (soil)&nbsp;
