            <% }else{ %>
                <input type='checkbox' class='filter_ckbx' id='oral_low' name='oral_low' onclick='update_sb()'\> Oral (low)&nbsp;
            <% } %>
            <% if(statusfltr.site.oral_med === 'on'){ %>
                <input type='checkbox' checked class='filter_ckbx' id='oral_med' name='oral_med' onclick='update_sb()'\> Oral (med)&nbsp;
