<style>
.search-box{
   height:100px;
   overflow:auto;
   margin:5px 0;
   padding:10px;
   background:lightyellow;
   border:1px solid black;
}
</style>
<%- include('../partials/header_html'); %>
<% var otids = JSON.parse(otid_list) %>
<% var gids = JSON.parse(gid_list) %>
<% var lineageids = JSON.parse(taxon_otid_obj) %>
<% var phage_id_list = JSON.parse(phage_id_list) %>
<% var phage_name_list = JSON.parse(phage_name_list) %>

<body>
  
  	<div id='wrapper'>
    <%- include('../partials/header_img'); %>
    <%- include('../partials/navbar'); %>
    <div class='page-content' >	
			 <h4 class='title text-info'>Result [<small>Search Text: <%= search_text %></small>]</h4>
<!-- 'HMT-'+("000" + otid).slice(-3);	 -->
	<div class='search-box'>
	  To Search Help Pages See <a href='https://wiki.homd.info'>https://wiki.homd.info</a>
    </div>
	<div class='search-box'>
	  Matching Taxon Names: (<%= Object.keys(lineageids).length %>)
	  <% n = 0 %>
	  <% for(otid in lineageids){ %>
		   <% n += 1 %>
		   <% taxidfullname = 'HMT-'+("000" + otid).slice(-3); %>
		   <br><%= parseInt(n) %>) <a href='/taxa/tax_description?otid=<%= otid %>'><%= taxidfullname %></a> 
		   <em><%= lineageids[otid].genus %> <%= lineageids[otid].species %></em>
	<% } %>
	</div>
	
	<div class='search-box'>
	  Matching Taxon-IDs: (<%= otids.length %>)
	  <% for(n in otids){ %>
	   <% otid = otids[n] %>
	   <% taxidfullname = 'HMT-'+("000" + otid).slice(-3); %>
	   <br><%= parseInt(n)+1 %>) <a href='/taxa/tax_description?otid=<%= otid %>'><%= taxidfullname %></a>
	<% } %>
	</div>
	
	<div class='search-box'>
	  Matching Seq-IDs: (<%= gids.length %>)
	  <% for(n in gids){ %>
		   <% gid = gids[n] %>
		   <br><%= parseInt(n)+1 %>) <a href='/genome/genome_description?gid=<%= gid %>'><%= gid %></a>
		<% } %>
    </div>
    
   
    <div class='search-box'>
	   Matching Phage-IDs: (<%= phage_id_list.length %>)
	  <% for(n in phage_id_list){ %>
		   <% pid = phage_id_list[n] %>
		   <br><%= parseInt(n)+1 %>) <a href='/phage/phagedesc?pid=<%= pid %>'><%= pid %></a>
		<% } %>
    </div>
    <div class='search-box'>
	  Matching Phage Names: (<%= phage_name_list.length %>)
	  <% for(n in phage_name_list){ %>
		   <% pid = phage_name_list[n] %>
		   <br><%= parseInt(n)+1 %>) <a href='/phage/phagedesc?pid=<%= pid %>'><%= pid %></a> 
	<% } %>
    </div>
    </div> <!-- end: page-content -->
    
    <div id='footer'>
         <%- include('../partials/footer'); %>
    </div>
  </div>     
</body>

</html>

<script type="text/javascript" src="/js/jquery-2.1.1.min.js"></script> 
<script type="text/javascript" src="/js/menu.js"></script>
<script>
  mainmenu()
</script>


