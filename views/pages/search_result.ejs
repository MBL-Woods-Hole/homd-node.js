<style>
.search-box{
   height:100px;
   overflow:auto;
   margin:0px 0px;
   padding:0px 10px;
   background:lightyellow;
   border:1px solid black;
}
#srch_txt{
   color:orange;
}
</style>
<%- include('../partials/header_html'); %>
<% var otids = JSON.parse(otid_list) %>
<% var gids = JSON.parse(gid_list) %>
<% var lineageids = JSON.parse(taxon_otid_obj) %>
<% var phage_id_list = JSON.parse(phage_id_list) %>


<body>
  
  	<div id='wrapper'>
    <%- include('../partials/header_img'); %>
    <%- include('../partials/navbar'); %>
    <div class='page-content' >	
			 <h4 class='title text-info'>Search Result [<small>Search String: <span id='srch_txt'>"<%= search_text %>"</span></small>]</h4>
<!-- 'HMT-'+("000" + otid).slice(-3);	 -->
	<div class=''>
	  To Search the Help Pages See <a href='https://wiki.homd.info'>https://wiki.homd.info</a>
    </div>
    
    <br>Matches from Bacterial Taxon Names: (<%= Object.keys(lineageids).length %>)
	<div class='search-box'>
	  <% n = 0 %>
	  <% for(otid in lineageids){ %>
		   <% n += 1 %>
		   <% taxidfullname = 'HMT-'+("000" + otid).slice(-3); %>
		   <br><%= parseInt(n) %>) <a href='/taxa/tax_description?otid=<%= otid %>'><%= taxidfullname %></a> 
		   <em><%= lineageids[otid] %></em>
	<% } %>
	</div>
    <hr> 
    Matches from Oral Taxon-ID Metadata: (<%= otids.length %>)	
	<div class='search-box'>
	  <% for(n in otids){ %>
	   <% otid = otids[n] %>
	   <% taxidfullname = 'HMT-'+("000" + otid).slice(-3); %>
	   <br><%= parseInt(n)+1 %>) <a href='/taxa/tax_description?otid=<%= otid %>'><%= taxidfullname %></a>
	<% } %>
	</div>
    <hr> 	
    Matches from Genome Metadata: (<%= gids.length %>)
	<div class='search-box'>
	  <% for(n in gids){ %>
		   <% gid = gids[n] %>
		   <br><%= parseInt(n)+1 %>) <a href='/genome/genome_description?gid=<%= gid %>'><%= gid %></a>
		<% } %>
    </div>
    
    <hr>   
    Matches from Phage Metadata: (<%= phage_id_list.length %>)
    <div class='search-box'>
	  <% for(n in phage_id_list){ %>
		   <% pid = phage_id_list[n] %>
		   <br><%= parseInt(n)+1 %>) <a href='/phage/phagedesc?pid=<%= pid %>'><%= pid %></a>
		<% } %>
    </div>

    </div> <!-- end: page-content -->
    
    <div id='footer'>
         <%- include('../partials/footer'); %>
    </div>
  </div>     
</body>

</html>

<script type="text/javascript" src="/js/jquery-2.1.1.min.js"></script> 
<script type="text/javascript" src="/js/menu.js"></script>
<script>
  mainmenu()
</script>


