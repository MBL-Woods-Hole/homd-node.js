  --btn-text:#f0f0f0;
  --srch-bg:#ccebc5;
  --text:black;
  --cancel-btn:#B22222;
  --div-bg016:#f0f0f0;   /* Pastel grey */
  
}
/* {box-sizing: border-box;} */
/* https://www.w3schools.com/colors/colors_monochromatic.asp */

.open-button {
  background: var(--bg);
  border: 1px solid red; /* var(--border2); */
  color: var(--black);
  padding: 5px 5px;
  border: none;
  cursor: pointer;
  /* opacity: 0.8; */
 /* 
 position: fixed;
  bottom: 23px;
  right: 28px;
  width: 280px;
 */
}
#field_select{
    background: var(--div-bg016);
    
}
#txt_srch{
   background: var(--div-bg016);
   
}
/* The popup form - hidden by default */
.form-popup {
  display: none;
  /* visibility: hidden; */
   position: fixed;
    top: 550px;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 70%;

  /* 
position: relative;
  top: 150px;
  left: 150px;
  width: 1500px;
  */
  border: 2px solid var(--border1);
  z-index: 9;
  background-color: var(--bg);
  padding:10px;
 
  
  
 
}


/* Desktops and laptops ----------- */
@media only screen 
and (min-width : 1224px) {
  /* Styles */
		.form-container {
			font-size:12px;
			/* width: 1500px; */
			/* padding: 10px; */
			/* background-color: white; */
		}
		
		/* Full-width input fields */
		.form-container input[type=text] {
